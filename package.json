{
  "name": "react-web-starter",
  "version": "1.0.0",
  "description": "React + Redux front-end scaffold.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "prettify": "find src -name '*.js' | xargs prettier --write",
    "lint": "./node_modules/.bin/eslint --fix 'src/**/*.js'",
    "webpack-dev": "webpack --config webpack.config.js --watch",
    "webpack-prod": "webpack --config webpack.production.config.js",
    "clean-web": "rimraf build/*",
    "start-dev-server": "./node_modules/.bin/nodemon --watch src",
    "dev":
      " npm run clean-web && concurrently --kill-others \"npm run webpack-dev\"  \"npm run start-dev-server\" ",
    "start": "node ./index.js",
    "build": "npm run clean-web && npm run webpack-prod",
    "prod": "npm run build && pm2 start ./index.js",
    "postinstall": "npm run build"
  },
  "lint-staged": {
    "src/**/*.js": ["prettier --write", "./node_modules/.bin/eslint --fix", "git add"],
    "src/**/*.css": ["prettier --write", "git add"]
  },
  "dependencies": {
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-plugin-idx": "2.2.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "compression": "1.7.1",
    "concurrently": "3.5.1",
    "css-loader": "0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "4.16.2",
    "file-loader": "1.1.5",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "icepick": "2.2.1",
    "idx": "2.2.0",
    "jquery": "3.2.1",
    "lint-staged": "5.0.0",
    "moment": "2.19.2",
    "node-sass": "4.7.2",
    "path": "0.12.7",
    "prettier": "1.8.2",
    "react": "16.1.1",
    "react-addons-shallow-compare": "15.6.2",
    "react-dates": "15.1.0",
    "react-dom": "16.1.1",
    "react-dropzone": "4.2.3",
    "react-icons": "2.2.7",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-select": "1.0.1",
    "react-toggle": "4.0.2",
    "redux": "3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "7.1.2",
    "redux-saga": "^0.16.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "uikit": "3.0.0-beta.35",
    "url-loader": "0.6.2",
    "webpack": "3.8.1"
  },
  "devDependencies": {
    "nodemon": "1.12.1"
  }
}
